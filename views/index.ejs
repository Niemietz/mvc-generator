<!DOCTYPE html>
<html>
	<head>
		<title><%= title %></title>
	</head>
	<body class="kind-steel-gradient color-block p-3 mx-auto">
		<!-- Font Awesome 5.15.1 -->
		<!-- <link rel="stylesheet" href="stylesheets/all.min.css"> -->
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/css/all.min.css">
		<!-- Bootstrap core CSS -->
		<!-- <link href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" rel="stylesheet"> -->
		<!-- Material Design Bootstrap 5 / MDB UI Kit 3.1.0 -->
		<link href="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/3.1.0/mdb.min.css" rel="stylesheet">

		<!-- Animate CSS -->
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css">

		<link rel="stylesheet" href="stylesheets/database.css">
		<link rel="stylesheet" href="stylesheets/language-chooser.css">
		<link rel="stylesheet" href="stylesheets/custom.css">

		<!-- JQuery -->
		<script type="text/javascript" src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
		<!-- Bootstrap core JavaScript and Popper 1.16.1 -->
		<!-- <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.bundle.min.js"></script> -->
		<!-- MDB core JavaScript -->
		<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/3.1.0/mdb.min.js"></script>

		<!-- Bootstrap Notify 3.1.3 -->
		<script type="text/javascript" src="javascripts/bootstrap-notify.js"></script>

		<script type="text/javascript" src="javascripts/common.js"></script>
		<script type="module" src="javascripts/tabs/page-adder.js"></script>
		<script type="module" src="javascripts/tabs/model-adder.js"></script>
		<script type="module" src="javascripts/tabs/database.js"></script>
		<script type="module" src="javascripts/views/language-chooser.js"></script>
		<script type="module" src="javascripts/tabs/framework-libs.js"></script>
		<script type="module" src="javascripts/views/index.js"></script>
		<div class="container-fluid">
			<div class="row">
				<div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
					<h5 style="text-transform: uppercase;">MVC Generator</h5>
				</div>
				<div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 pb-3" style="
					border: 1px solid #cdccc9;
					border-radius: 5px;
				">
					<div class="languages">
						<div id="php" class="language mdb-tooltip" value="1" data-mdb-toggle="tooltip" data-mdb-placement="top" title="PHP" checked>
							<i class="fab fa-php"></i>
							<span>PHP</span>
						</div>
						<div id="java" class="language mdb-tooltip" value="2" data-mdb-toggle="tooltip" data-mdb-placement="top" title="Java">
							<i class="fab fa-java"></i>
							<span>Java</span>
						</div>
						<div id="android" class="language mdb-tooltip" value="3" data-mdb-toggle="tooltip" data-mdb-placement="top" title="Android">
							<i class="fab fa-android"></i>
							<span>Android</span>
						</div>
					</div>
				</div>
			</div>
			<div class="row mt-3">				
				<div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 pb-3" style="
						border: 1px solid #cdccc9;
						border-radius: 5px;
					">
					<header>
						<ul class="nav nav-tabs" id="myTab" role="tablist" style="border-top-left-radius: .25rem; border-top-right-radius: .25rem;">
							<li class="nav-item">
								<a class="nav-link active" id="database-tab" data-mdb-toggle="tab" href="#database" role="tab" aria-controls="database" aria-selected="false">Banco de Dados</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" id="models-tab" data-mdb-toggle="tab" href="#models" role="tab" aria-controls="models" aria-selected="false">Classes (Models)</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" id="pages-tab" data-mdb-toggle="tab" href="#pages" role="tab" aria-controls="pages" aria-selected="false">PÃ¡ginas</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" id="frameworks-libs-tab" data-mdb-toggle="tab" href="#frameworks-libs" role="tab" aria-controls="frameworks-libs" aria-selected="false">Frameworks e Bibliotecas</a>
							</li>
						</ul>
					</header>
					<div class="tab-content" id="myTabContent">
						<div class="tab-pane show active" id="database" role="tabpanel" aria-labelledby="database-tab">
							<%- include loading.ejs %>
							<%- include tabs/database.ejs %>
						</div>
						<div class="tab-pane fade" id="models" role="tabpanel" aria-labelledby="models-tab">
							<%- include loading.ejs %>
							<%- include tabs/models.ejs %>
						</div>
						<div class="tab-pane fade" id="pages" role="tabpanel" aria-labelledby="pages-tab">
							<%- include loading.ejs %>
							<%- include tabs/pages.ejs %>
						</div>
						<div class="tab-pane fade" id="frameworks-libs" role="tabpanel" aria-labelledby="frameworks-libs-tab">
							<%- include loading.ejs %>
							<%- include tabs/frameworks-libs.ejs %>
						</div>
					</div>
				</div>
			</div>
			<div class="row mt-3">
				<div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
					<!--<a id="back" class="btn peach-gradient btn-rounded waves-effect">Voltar</a>-->
					<a id="generate-files" class="btn btn-secondary btn-rounded waves-effect">Gerar arquivos</a>
				</div>
			</div>
		</div>
	</body>
</html>