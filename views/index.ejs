		<script type="module" src="javascripts/tabs/framework-libs.js"></script>
		<script type="module" src="javascripts/views/index.js"></script>
		<div class="container-fluid">
			<div class="row">
				<div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
					<header>
						<ul class="nav nav-tabs" id="myTab" role="tablist" style="border-top-left-radius: .25rem; border-top-right-radius: .25rem;">
							<li class="nav-item">
								<a class="nav-link active" id="choose-language-tab" data-mdb-toggle="tab" href="#choose-language" role="tab" aria-controls="choose-language" aria-selected="true">Linguagem</a>
							</li>
							<li class="nav-item">
								<a class="nav-link" id="models-tab" data-mdb-toggle="tab" href="#models" role="tab" aria-controls="models" aria-selected="false">Models</a>
							</li>
							<li class="nav-item">
